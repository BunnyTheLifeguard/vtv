{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es2016",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "types": ["vite-plugin-pages/client", "unplugin-icons/types/vue"]
  },
  "paths": {
    "~/*": ["src/*"]
  },
  "exclude": ["dist", "node_modules"]
}
